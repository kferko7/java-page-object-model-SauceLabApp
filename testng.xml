<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SuiteTNG">
    <parameter name="browser" value="chrome"/>

    <listeners>
        <listener class-name="listeners.Listeners" />
    </listeners>

    <test name="Login">
        <classes>
            <class name="Login.LoginTests"/>
        </classes>
    </test>

    <test name="Backpack">
        <classes>
            <class name="Backpack.BackpackTests"/>
        </classes>
    </test>

    <test name="Dropdown">
        <classes>
            <class name="Dropdown.DropdownTests"/>
        </classes>
    </test>

    <test name="Tshirt">
        <classes>
            <class name="Tshirt.TshirtTests"/>
        </classes>
    </test>




</suite>